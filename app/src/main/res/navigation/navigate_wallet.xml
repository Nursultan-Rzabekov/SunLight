<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/walletFragment"
    android:id="@+id/navigate_wallet">

    <fragment android:id="@+id/walletFragment"
        android:name="com.example.sunlightdesign.ui.screens.wallet.WalletFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_wallet">

        <action
            android:id="@+id/action_walletFragment_to_withdrawFragment"
            app:destination="@id/withdrawFragment" />
    </fragment>

    <fragment android:id="@+id/withdrawFragment"
        android:name="com.example.sunlightdesign.ui.screens.wallet.withdraw.WithdrawFragment"
        tools:layout="@layout/fragment_withdraw">


    </fragment>

</navigation>